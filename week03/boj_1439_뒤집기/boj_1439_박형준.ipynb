   "source": [
    "S = input()\n",
    "# 10, 01 찾기\n",
    "n1 = S.count(\"10\")\n",
    "n2 = S.count(\"01\")\n",
    "n3 = n1 + n2\n",
    "# 10 과 01 개수가 짝수인 경우 홀수인 경우 \n",
    "if n3 % 2 == 0 :\n",
    "    n = n3 // 2\n",
    "else:\n",
    "    n = (n3 // 2) + 1\n",
    "\n",
    "print(n)"
   ]
  }
