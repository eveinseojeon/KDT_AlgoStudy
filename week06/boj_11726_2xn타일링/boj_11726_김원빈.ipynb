    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "dp = [0] * n\n",
    "\n",
    "dp[0] = 1\n",
    "dp[1] = 2\n",
    "for i in range(2, n):\n",
    "    dp[i] = dp[i-1] + dp[i-2]\n",
    "    \n",
    "print(dp[n-1] % 10007)"
   ]
  }
