   "source": [
    "import sys\n",
    "read = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "nums = list(map(int, input().split()))\n",
    "\n",
    "dp = [0 for _ in range(n)]\n",
    "dp[0] = nums[0]\n",
    "\n",
    "for i in range(1, n):\n",
    "    dp[i] = max(nums[i], nums[i] + dp[i-1])\n",
    "\n",
    "print(max(dp))"
   ]
