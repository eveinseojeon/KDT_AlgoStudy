    "import sys\n",
    "input = sys.stdin.readline\n",
    "\n",
    "n = int(input())\n",
    "temp = list(map(int, input().split()))\n",
    "\n",
    "temp.sort()\n",
    "\n",
    "result = 0\n",
    "for i in range(n):\n",
    "    result += temp[i] * (n - i)\n",
    "\n",
    "print(result)"
   ]
