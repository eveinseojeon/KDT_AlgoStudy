   "source": [
    "n = int(input())\n",
    "p_list = list(map(int, input().split()))\n",
    "p_list = sorted(p_list) # 인출 시간이 짧은 사람부터 오름차순 정렬\n",
    "\n",
    "answer = 0\n",
    "for i in range(n):\n",
    "    answer += p_list[i] * (n-i)\n",
    "\n",
    "print(answer)"
   ]
  }
