   ],
   "source": [
    "n = int(input())\n",
    "boxes = list(map(int, input().split()))\n",
    "\n",
    "d = [1 for _ in range(n)]\n",
    "\n",
    "for i in range(1, n):\n",
    "    for j in range(i):\n",
    "        if boxes[j] < boxes[i]:\n",
    "            d[i] = max(d[j] + 1, d[i])\n",
    "\n",
    "print(max(d))"
   ]
  }
