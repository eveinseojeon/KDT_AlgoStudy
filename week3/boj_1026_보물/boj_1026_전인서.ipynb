   "source": [
    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "B = list(map(int, input().split()))\n",
    "\n",
    "S = 0\n",
    "for _ in range(N):\n",
    "    S += min(A)*max(B)\n",
    "    A.remove(min(A))\n",
    "    B.remove(max(B))\n",
    "\n",
    "print(S)"
