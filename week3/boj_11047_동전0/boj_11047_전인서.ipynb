   ],
   "source": [
    "N, K = map(int, input().split())\n",
    "coins = [int(input()) for _ in range(N)][::-1] # 오름차순으로 입력된 값들을 뒤집어서 내림차순 정렬\n",
    "\n",
    "count = 0\n",
    "for i in range(N):\n",
    "    count += K//coins[i]\n",
    "    K -= (K//coins[i])*coins[i]\n",
    "    \n",
    "    if K==0: # \n",
    "        break\n",
    "\n",
    "print(count)"
   ]
